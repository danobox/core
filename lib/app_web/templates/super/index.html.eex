<section class="phx-hero">
  <p>This is the superuser dashboard.</p>
</section>

<section>
  <h2>Superusers</h2>
  <ul>
    <%= for user <- @supers do %>
      <li>
        <%= user.email %>
      </li>
    <% end %>
  </ul>
</section>

<section>
  <h2>Normal Users</h2>
  <ul>
    <%= for user <- @users do %>
      <li>
        <%= user.email %>
      </li>
    <% end %>
  </ul>
</section>
